[server]
check-response-ttl=no
ratelimit-interval-usec=0
use-ipv4=yes
use-ipv6=no
allow-interfaces=lo,eth0,veth
enable-dbus=no
#host-name=mesh-app
#browse-domains=mesh-app.local,backend.mesh-app.local,frontend.mesh-app.local

[wide-area]
enable-wide-area=no

[publish]
disable-publishing=no
disable-user-service-publishing=no
add-service-cookie=no

[reflector]
enable-reflector=no